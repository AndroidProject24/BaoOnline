<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StateLayout">
        <attr name="errorDrawable" format="reference" />
        <attr name="emptyDrawable" format="reference" />
        <attr name="progressView" format="reference" />
    </declare-styleable>
    <declare-styleable name="AVLoadingIndicatorView">
        <attr name="minWidth" format="dimension" />
        <attr name="maxWidth" format="dimension"/>
        <attr name="minHeight" format="dimension" />
        <attr name="maxHeight" format="dimension"/>
        <attr name="indicatorName" format="string"/>
        <attr name="indicatorColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="ParticleView">
        <attr name="pv_host_text" format="string"/>
        <attr name="pv_particle_text" format="string"/>
        <attr name="pv_particle_text_size" format="dimension"/>
        <attr name="pv_text_color" format="color"/>
        <attr name="pv_background_color" format="color"/>
    </declare-styleable>
    <declare-styleable name="AppThemeAttributes">
        <attr name="selectableRectDrawable" format="reference"/>
        <attr name="selectableRectDrawableInverse" format="reference"/>
        <attr name="selectableRectDrawableColored" format="reference"/>
        <attr name="selectableRoundedRectDrawable" format="reference"/>
        <attr name="selectableRoundedRectDrawableInverse" format="reference"/>
        <attr name="selectableRoundedRectDrawableColored" format="reference"/>
    </declare-styleable>
</resources>